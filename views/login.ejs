<!DOCTYPE html>

<html class="no-js">
  <head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Resource Wall</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">


  <link href='https://fonts.googleapis.com/css?family=Roboto:400,300,600,400italic,700' rel='stylesheet' type='text/css'>
  <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>

  <!-- Animate.css -->
  <link rel="stylesheet" href="/css/animate.css">
  <!-- Icomoon Icon Fonts-->
  <link rel="stylesheet" href="/css/icomoon.css">
  <!-- Bootstrap  -->
  <link rel="stylesheet" href="/css/bootstrap.css">
  <!-- Owl Carousel -->
  <link rel="stylesheet" href="/css/owl.carousel.min.css">
  <link rel="stylesheet" href="/css/owl.theme.default.min.css">
  <!-- Theme style  -->
  <link rel="stylesheet" href="/css/style.css">

  <!-- Modernizr JS -->
  <script src="/scripts/modernizr-2.6.2.min.js"></script>
  <!-- FOR IE9 below -->
  <!--[if lt IE 9]>
  <script src="scripts/respond.min.js"></script>
  <![endif]-->

  </head>

  <body>

    <div id="fh5co-page">
    <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle"><i></i></a>
    <aside id="fh5co-aside" role="complementary" class="border js-fullheight">

      <h1 id="fh5co-logo"><img src="/images/logo.png"></a></h1>
      <nav id="fh5co-main-menu" role="navigation">
      </nav>

        <div class="fh5co-footer">
        <small>
          <span>&copy; 2017 Made by .</span>
          <span>Designed by <a href="http://freehtml5.co/" target="_blank">FreeHTML5.co</a> </span>
        </small>
      </div>
    </aside>
  </div>

<div id="fh5co-main">
  <div class="fh5co-narrow-content">
    <h2 class="fh5co-heading animate-box" align = "center" data-animate-effect = "fadeInLeft">Welcome to Resource Wall</h2>
    <div class="row animate-box" data-animate-effect="fadeInLeft">
      <div class="container">
        <% if (errors.length) { %>
          <% errors.forEach((error) => { %>
            <h3><%= error %>!</h3>
          <% }) %>
        <% } %>
        <form action="/users/login" method="POST">
          <h1>Login</h1>
          <div class="input-group">
            <label for="login-email"></label>
            <input id="login-email" class="form-control" placeholder = "Enter your email" name="email">
          </div>
          <div class="input-group">
            <label for="login-password"></label>
            <input id="login-password" class="form-control" placeholder = "Enter your password" name="password" type="password">
          </div>
          <button class="btn btn-primary">Login</button>
        </form>
        <br>
        <form action="/users" method="POST">
          <h1>Register</h1>
          <div class="input-group">
            <label for="username"></label>
            <input id="username" class="form-control" placeholder = "Enter your username" name="username">
          </div>
          <div class="input-group">
            <label for="register-email"></label>
            <input id="register-email" class="form-control" placeholder = "Enter your email" name="email">
          </div>
          <div class="input-group">
            <label for="register-password"></label>
            <input id="register-password" class="form-control" name="password" placeholder = "Enter your password" type="password">
          </div>
          <button class="btn btn-primary">Create Account</button>
        </form>
      </div>
    </div>
  </div>
</div>

  <script src="/scripts/jquery.min.js"></script>
  <!-- jQuery Easing -->
  <script src="/scripts/jquery.easing.1.3.js"></script>
  <!-- Bootstrap -->
  <script src="/scripts/bootstrap.min.js"></script>
  <!-- Carousel -->
  <script src="/scripts/owl.carousel.min.js"></script>
  <!-- Stellar -->
  <script src="/scripts/jquery.stellar.min.js"></script>
  <!-- Waypoints -->
  <script src="/scripts/jquery.waypoints.min.js"></script>
  <!-- Counters -->
  <script src="/scripts/jquery.countTo.js"></script>



  <!-- MAIN JS -->
  <script src="/scripts/main.js"></script>

  </body>
</html>
