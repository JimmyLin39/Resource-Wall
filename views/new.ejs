	<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>Resource Wall</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<link href='https://fonts.googleapis.com/css?family=Roboto:400,300,600,400italic,700' rel='stylesheet' type='text/css'>
	<link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>

	<!-- Animate.css -->
	<link rel="stylesheet" href="/css/animate.css">
	<!-- Icomoon Icon Fonts-->
	<link rel="stylesheet" href="/css/icomoon.css">
	<!-- Bootstrap  -->
	<link rel="stylesheet" href="/css/bootstrap.css">
	<!-- Owl Carousel -->
	<link rel="stylesheet" href="/css/owl.carousel.min.css">
	<link rel="stylesheet" href="/css/owl.theme.default.min.css">
	<!-- Theme style  -->
	<link rel="stylesheet" href="/css/style.css">

	<!-- Modernizr JS -->
	<script src="/scripts/modernizr-2.6.2.min.js"></script>
	<!-- FOR IE9 below -->
	<!--[if lt IE 9]>
	<script src="/scripts/respond.min.js"></script>
	<![endif]-->

	</head>
	<body>

	<div id="fh5co-page">
		<a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle"><i></i></a>
		<aside id="fh5co-aside" role="complementary" class="border js-fullheight">

			<h1 id="fh5co-logo"><img src="/images/logo.png"></a></h1>
			<nav id="fh5co-main-menu" role="navigation">
				<ul>
					<li><a href="/resources">Home Page</a></li>
          <li class="fh5co-active"><a href="/resources/new">new!</a></li>
					<li><a href="/users/<%=user_id%>/profile">My Profile</a></li>
					<li>
            <form method="POST" id="logoutbtn" action="/users/logout">
              <!-- <a href="/users/logout"> -->
              <input type="submit" class="logoutbtn" name="logout" value="LOGOUT">
            </form>
          </li>
				</ul>
			</nav>

			<div class="fh5co-footer">
        <small>
          <span>&copy; 2017 Made by .</span>
          <span>Designed by <a href="http://freehtml5.co/" target="_blank">FreeHTML5.co</a> </span>
        </small>
			</div>

		</aside>

		<div id="fh5co-main">
      <form method = "GET" action="/resources/search" >
      <input type="text" name="search" placeholder="Search..">
        <button>Submit</button>
      </form>
			<div class="fh5co-narrow-content">
				<h2 class="fh5co-heading animate-box" data-animate-effect="fadeInLeft">
          Create a new resource!
        </h2>
				<div class="row animate-box" data-animate-effect="fadeInLeft">
          <div class="container">
            <form method="POST" action="/resources">
              <div class="input-group">
                <input type="text" name="title" class="form-control" placeholder="TITLE" aria-describedby="basic-addon1">
              </div>
              <div class="input-group">
                <input type="text" name="url" class="form-control" placeholder="LINK http://" aria-describedby="basic-addon1">
              </div>
              <div class="input-group">
                <input type="text" name="description" class="form-control" placeholder="DESCRIPTION" aria-describedby="basic-addon1">
              </div>
              <div class="input-group">
                <input type="text" name="tag" class="form-control" placeholder="TAG" aria-describedby="basic-addon1">
              </div>
              <button class="btn btn-primary">Create!</button>
            </form>
          </div>
				</div>
			</div>
		</div>
	</div>

	<!-- jQuery -->
	<script src="/scripts/jquery.min.js"></script>
	<!-- jQuery Easing -->
	<script src="/scripts/jquery.easing.1.3.js"></script>
	<!-- Bootstrap -->
	<script src="/scripts/bootstrap.min.js"></script>
	<!-- Carousel -->
	<script src="/scripts/owl.carousel.min.js"></script>
	<!-- Stellar -->
	<script src="/scripts/jquery.stellar.min.js"></script>
	<!-- Waypoints -->
	<script src="/scripts/jquery.waypoints.min.js"></script>
	<!-- Counters -->
	<script src="/scripts/jquery.countTo.js"></script>



	<!-- MAIN JS -->
	<script src="/scripts/main.js"></script>

	</body>
</html>
